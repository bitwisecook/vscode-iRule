{
  "if": {
    "prefix": "if",
    "body": "if {${1:condition}} {\n\t${2}\n}\n",
    "description": "if...",
    "scope": "source.iRule"
  },
  "ifelse": {
    "prefix": "if",
    "body": "if {${1:condition}} {\n\t${2}\n} else {\n\t${3}\n}\n",
    "description": "if...else",
    "scope": "source.iRule"
  },
  "ifelseif": {
    "prefix": "if",
    "body": "if {${1:condition}} {\n\t${2}\n} elseif {${3:condition}} {\n\t${4}\n}\n",
    "description": "if...elseif",
    "scope": "source.iRule"
  },
  "for": {
    "prefix": "for",
    "body": "for {set ${1:i} 0} {${1} < ${2:10}} {incr ${1}} {\n\t${3}\n}\n",
    "description": "for...",
    "scope": "source.iRule"
  },
  "foreach": {
    "prefix": "foreach",
    "body": "foreach ${1:var} ${2:list} {\n\t${3}\n}\n",
    "description": "foreach...",
    "scope": "source.iRule"
  },
  "proc": {
    "prefix": "proc",
    "body": "proc ${1:name} {${2:args}} {\n\t${3}\n}\n",
    "description": "proc...",
    "scope": "source.iRule"
  },
  "switch": {
    "prefix": "switch",
    "body": "switch ${1|-exact,-glob,-regex|} -- ${2:var} {\n\t${3:\"match\"} {\n\t\t${4}\n\t}\n\tdefault {\n\t\t${5}\n\t}\n}\n",
    "description": "switch...",
    "scope": "source.iRule"
  },
  "while": {
    "prefix": "while",
    "body": "while {${1:condition}} {\n\t${2}\n}\n",
    "description": "while...",
    "scope": "source.iRule"
  }
}